{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "primary": "#a1c9fd",
    "primaryLight": "#37618e",
    "primaryDark": "#a1c9fd",
    "secondary": "#bbc7db",
    "secondaryLight": "#535f70",
    "secondaryDark": "#bbc7db",
    "tertiary": "#d7bde4",
    "tertiaryLight": "#6b5778",
    "tertiaryDark": "#d7bde4",
    "error": "#ffb4ab",
    "errorLight": "#ba1a1a",
    "errorDark": "#ffb4ab",
    "surface": "#111418",
    "surfaceLight": "#f8f9ff",
    "surfaceDark": "#111418",
    "surfaceVariant": "#43474e",
    "onPrimary": "#003259",
    "onSecondary": "#253141",
    "onTertiary": "#3b2947",
    "onError": "#690005",
    "onSurface": "#e1e2e8",
    "onSurfaceVariant": "#c3c6cf",
    "background": "#111418",
    "onBackground": "#e1e2e8"
  },
  "theme": {
    "primary": {
      "dark": "primaryDark",
      "light": "primary"
    },
    "secondary": {
      "dark": "secondaryDark",
      "light": "secondary"
    },
    "accent": {
      "dark": "tertiaryDark",
      "light": "tertiary"
    },
    "error": {
      "dark": "errorDark",
      "light": "error"
    },
    "warning": {
      "dark": "errorLight",
      "light": "errorLight"
    },
    "success": {
      "dark": "tertiary",
      "light": "tertiary"
    },
    "info": {
      "dark": "primaryDark",
      "light": "primary"
    },
    "text": {
      "dark": "onSurface",
      "light": "onBackground"
    },
    "textMuted": {
      "dark": "onSurfaceVariant",
      "light": "onSurfaceVariant"
    },
    "background": {
      "dark": "none",
      "light": "none"
    },
    "backgroundPanel": {
      "dark": "background",
      "light": "background"
    },
    "backgroundElement": {
      "dark": "surfaceVariant",
      "light": "surface"
    },
    "border": {
      "dark": "surfaceVariant",
      "light": "onSurfaceVariant"
    },
    "borderActive": {
      "dark": "primaryDark",
      "light": "primary"
    },
    "borderSubtle": {
      "dark": "background",
      "light": "background"
    },
    "diffAdded": {
      "dark": "tertiary",
      "light": "tertiary"
    },
    "diffRemoved": {
      "dark": "error",
      "light": "error"
    },
    "diffContext": {
      "dark": "onSurfaceVariant",
      "light": "onSurfaceVariant"
    },
    "diffHunkHeader": {
      "dark": "onSurfaceVariant",
      "light": "onSurfaceVariant"
    },
    "diffHighlightAdded": {
      "dark": "tertiaryLight",
      "light": "tertiaryLight"
    },
    "diffHighlightRemoved": {
      "dark": "errorLight",
      "light": "errorLight"
    },
    "diffAddedBg": {
      "dark": "surfaceDark",
      "light": "surfaceLight"
    },
    "diffRemovedBg": {
      "dark": "surfaceDark",
      "light": "surfaceLight"
    },
    "diffContextBg": {
      "dark": "surface",
      "light": "surface"
    },
    "diffLineNumber": {
      "dark": "onSurfaceVariant",
      "light": "onSurfaceVariant"
    },
    "diffAddedLineNumberBg": {
      "dark": "surfaceDark",
      "light": "surfaceLight"
    },
    "diffRemovedLineNumberBg": {
      "dark": "surfaceDark",
      "light": "surfaceLight"
    },
    "markdownText": {
      "dark": "onSurface",
      "light": "onBackground"
    },
    "markdownHeading": {
      "dark": "primaryDark",
      "light": "primary"
    },
    "markdownLink": {
      "dark": "secondaryDark",
      "light": "secondary"
    },
    "markdownLinkText": {
      "dark": "tertiaryDark",
      "light": "tertiary"
    },
    "markdownCode": {
      "dark": "tertiary",
      "light": "tertiary"
    },
    "markdownBlockQuote": {
      "dark": "onSurfaceVariant",
      "light": "onSurfaceVariant"
    },
    "markdownEmph": {
      "dark": "errorLight",
      "light": "errorLight"
    },
    "markdownStrong": {
      "dark": "primaryLight",
      "light": "primaryDark"
    },
    "markdownHorizontalRule": {
      "dark": "onSurfaceVariant",
      "light": "onSurfaceVariant"
    },
    "markdownListItem": {
      "dark": "primaryDark",
      "light": "primary"
    },
    "markdownListEnumeration": {
      "dark": "tertiaryDark",
      "light": "tertiary"
    },
    "markdownImage": {
      "dark": "secondaryDark",
      "light": "secondary"
    },
    "markdownImageText": {
      "dark": "tertiaryDark",
      "light": "tertiary"
    },
    "markdownCodeBlock": {
      "dark": "onSurface",
      "light": "onBackground"
    },
    "syntaxComment": {
      "dark": "onSurfaceVariant",
      "light": "onSurfaceVariant"
    },
    "syntaxKeyword": {
      "dark": "secondaryDark",
      "light": "secondary"
    },
    "syntaxFunction": {
      "dark": "primaryDark",
      "light": "primary"
    },
    "syntaxVariable": {
      "dark": "tertiaryDark",
      "light": "tertiary"
    },
    "syntaxString": {
      "dark": "tertiary",
      "light": "tertiary"
    },
    "syntaxNumber": {
      "dark": "tertiaryLight",
      "light": "tertiaryLight"
    },
    "syntaxType": {
      "dark": "tertiaryDark",
      "light": "tertiary"
    },
    "syntaxOperator": {
      "dark": "secondaryDark",
      "light": "secondary"
    },
    "syntaxPunctuation": {
      "dark": "onSurface",
      "light": "onBackground"
    }
  }
}
